<h1>crear cuenta</h1>

<%= form_with(model: @user, local: true) do |f| %>

    <div>
        <%= f.label :email, 'mail' %>
        <%= f.email_field :email %>
    </div>

    <div>
        <%= f.label :password, 'pass' %>
        <%= f.password_field :password %>
    </div>

    <%= f.submit 'guardar' %>

<% end %>